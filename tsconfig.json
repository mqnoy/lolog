{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
}